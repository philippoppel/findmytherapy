{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm vercel-build",
  "build": {
    "env": {
      "ALLOW_INCOMPLETE_ENV": "1"
    }
  },
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ]
}
